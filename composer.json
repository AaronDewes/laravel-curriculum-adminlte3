{
    "name": "joachimdieterich/curriculum",
    "type": "project",
    "description": "curriculum (laravel)",
    "version": "0.1.0",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "aacotroneo/laravel-saml2": "*",
        "adldap2/adldap2-laravel": "^6.0",
        "barryvdh/laravel-snappy": "^0.4.6",
        "bugsnag/bugsnag-laravel": "^2.15",
        "darkaonline/l5-swagger": "5.8.*",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "profburial/wkhtmltopdf-binaries-osx": "0.12.1",
        "intervention/image": "^2.4",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.4",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "laravolt/avatar": "^2.2",
        "lightsaml/lightsaml": "^1.4",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.3",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "phpunit/phpunit-selenium": ">=1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "authors" : [{
                    "name" : "Joachim Dieterich",
                    "email" : "mail@joachimdieterich.com",
                    "homepage": "http://www.curriculumonline.de",
                    "role": "Developer"
		}
	]
}
